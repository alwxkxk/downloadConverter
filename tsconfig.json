{
  "compileOnSave": true,
  "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "noImplicitAny": true,
      "removeComments": false,
      "preserveConstEnums": true,
      "outDir": "dist",
      "sourceMap": false
  },
  "include": [
      "src/*"
  ],
  "exclude": [
      "node_modules",
      ".sonarlint"
  ]
}

